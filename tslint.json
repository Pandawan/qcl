{
    "extends": ["tslint:recommended", "tslint-config-standard", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts",
            "lib/**/*.ts"
        ]
    },
    "rules": {
        "no-console": {
            "severity": "off"
        },
        "no-null-keyword": {
            "severity": "warning"
        }
    }
}
